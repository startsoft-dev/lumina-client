{
  "name": "@startsoft/lumina",
  "version": "0.0.2",
  "description": "Comprehensive React library for Laravel backends with full CRUD, pagination, soft deletes, and multi-tenant support. Works on web and React Native.",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "react-native": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "react-native": "./src/index.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "keywords": [
    "react",
    "react-native",
    "laravel",
    "crud",
    "hooks",
    "api",
    "typescript",
    "multi-tenant",
    "pagination",
    "soft-delete",
    "react-query",
    "tanstack-query",
    "frontend",
    "backend",
    "fullstack",
    "lumina"
  ],
  "author": "Bruno Cipolla <bruno@startsoft.com.br>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/startsoft-dev/lumina-client.git",
    "directory": "packages/@startsoft/lumina"
  },
  "bugs": {
    "url": "https://github.com/startsoft-dev/lumina-client/issues"
  },
  "homepage": "https://github.com/startsoft-dev/lumina-client/tree/main/packages/@startsoft/lumina#readme",
  "scripts": {
    "build": "vite build",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^5.0.0",
    "axios": "^1.0.0",
    "react": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "react-dom": {
      "optional": true
    },
    "@react-native-async-storage/async-storage": {
      "optional": true
    }
  },
  "devDependencies": {
    "@tanstack/react-query": "^5.90.20",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.3.1",
    "axios": "^1.13.5",
    "clsx": "^2.1.1",
    "cogent-js": "^2.0.0",
    "jsdom": "^25.0.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tailwind-merge": "^3.4.0",
    "typescript": "^5.0.0",
    "vite": "^6.0.1",
    "vite-plugin-dts": "^4.0.0",
    "vitest": "^3.2.4"
  }
}
